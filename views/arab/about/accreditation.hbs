    <!-- Site Stylesheet -->
    <link rel="stylesheet" href="assets/css/app.css">

</head>

<body class="sticky-header">
    <div id="edublink-preloader">
        <div class="loading-spinner">
            <div class="preloader-spin-1"></div>
            <div class="preloader-spin-2"></div>
        </div>
        <div class="preloader-close-btn-wraper">
            <span class="btn btn-primary preloader-close-btn">
                Cancel Preloader</span>
        </div>
    </div>

    <div id="main-wrapper" class="main-wrapper">
        <!--=====================================-->
        <!--=        Header Area Start       	=-->
        <!--=====================================-->
        {{>mainheader}}
        <!--=====================================-->
        <!--=       Breadcrumb Area Start      =-->
        <!--=====================================-->

        <div class="edu-breadcrumb-area">
            <div class="container">
                <div class="breadcrumb-inner">
                    <div class="page-title">
                        <h1 class="title">الاتّفاقيّات </h1>
                    </div>
                    <ul class="edu-breadcrumb" dir="rtl">
                        <li class="breadcrumb-item"><a href="/">الرئيسية</a></li>
                        <li class="separator"><i class="icon-angle-left"></i></li>
                        <li class="breadcrumb-item"><a href="#">عن الجامعة</a></li>
                        <li class="separator"><i class="icon-angle-left"></i></li>
                        <li class="breadcrumb-item active" aria-current="page"> الاتّفاقيّات </li>
                    </ul>
                </div>
            </div>
            <ul class="shape-group">
                <li class="shape-1">
                    <span></span>
                </li>
                <li class="shape-2 scene"><img data-depth="2" src="assets/images/about/shape-13.png" alt="shape"></li>
                <li class="shape-3 scene"><img data-depth="-2" src="assets/images/about/shape-15.png" alt="shape"></li>
                <li class="shape-4">
                    <span></span>
                </li>
                <li class="shape-5 scene"><img data-depth="2" src="assets/images/about/shape-07.png" alt="shape"></li>
            </ul>
        </div>
        <!--=====================================-->
        <!--=    Content   Area Start       	=-->
        <!--=====================================-->

        <section class="edu-section-gap course-details-area bg-lighten01">
            <div class="container">
                <div class="row row--30" dir="rtl">



                    <div class="accordion edu-accordion">

                        <div class="row">
                            <div class="col-lg-6" style="margin-bottom: 20px;">
                                <h2 class="title color-secondary">الدوليّة</h2>


                                <div class="accordion-item no-collapsed bg-lighten02" dir="rtl" style="border-radius:10px;">
                                    <h4 class="accordion-header color-primary " style="padding: 10px;">
                                        جامعة الأزهر الشريف، القاهرة، جمهوريّة مصر العربيّة
                                    </h4>
                                </div>
                                <div class="accordion-item no-collapsed bg-lighten02" dir="rtl" style="border-radius: 10px;">
                                    <h4 class="accordion-header color-primary " style="padding: 10px;">
                                        جامعة الزيتونة، تونس
                                    </h4>
                                </div>
                                <div class="accordion-item no-collapsed bg-lighten02" dir="rtl" style="border-radius: 10px;">
                                    <h4 class="accordion-header color-primary " style="padding: 10px;">
                                        الجامعة القاسميّة، الشارقة، الإمارات العربيّة المتّحدة
                                    </h4>
                                </div>
                                <div class="accordion-item no-collapsed bg-lighten02" dir="rtl" style="border-radius: 10px;">
                                    <h4 class="accordion-header color-primary " style="padding: 10px;">
                                        جامعة العلوم الإسلاميّة العالميّة، الأردن
                                    </h4>
                                </div>
                                <div class="accordion-item no-collapsed bg-lighten02" dir="rtl" style="border-radius: 10px;">
                                    <h4 class="accordion-header color-primary " style="padding: 10px;">
                                        جامعة الأحقاف، اليمن
                                    </h4>
                                </div>
                                <div class="accordion-item no-collapsed bg-lighten02" dir="rtl" style="border-radius: 10px;">
                                    <h4 class="accordion-header color-primary " style="padding: 10px;">
                                        كليّة الدعوة الإسلاميّة، ليبيا
                                    </h4>
                                </div>
                                <div class="accordion-item no-collapsed bg-lighten02" dir="rtl" style="border-radius: 10px;">
                                    <h4 class="accordion-header color-primary " style="padding: 10px;">
                                        جامعة العلوم الإسلاميّة الماليزيّة، ماليزيا
                                    </h4>
                                </div>
                                <div class="accordion-item no-collapsed bg-lighten02" dir="rtl" style="border-radius: 10px;">
                                    <h4 class="accordion-header color-primary " style="padding: 10px;">
                                        الجامعة الإفريقيّة العربيّة الإسلاميّة، سنغال
                                    </h4>
                                </div>
                                <div class="accordion-item no-collapsed bg-lighten02" dir="rtl" style="border-radius: 10px;">
                                    <h4 class="accordion-header color-primary " style="padding: 10px;">
                                        دار المصطفى للدراسات الإسلاميّة، تريم، اليمن
                                    </h4>
                                </div>
                                <div class="accordion-item no-collapsed bg-lighten02" dir="rtl" style="border-radius: 10px;">
                                    <h4 class="accordion-header color-primary " style="padding: 10px;">
                                        جامعة أسكدار، تركيا
                                    </h4>
                                </div>

                            </div>
                            <div class="col-lg-6">
                                <h2 class="title color-secondary">الهنديّة</h2>
                                <div class="accordion-item no-collapsed bg-lighten02" dir="rtl" style="border-radius: 10px;">
                                    <h4 class="accordion-header color-primary " style="padding: 10px;">
                                        جامعة عليكراه الإسلاميّة، عليكراه
                                    </h4>
                                </div>
                                <div class="accordion-item no-collapsed bg-lighten02" dir="rtl" style="border-radius: 10px;">
                                    <h4 class="accordion-header color-primary " style="padding: 10px;">
                                        الجامعة الملّيّة الإسلاميّة، دلهي
                                    </h4>
                                </div>
                                <div class="accordion-item no-collapsed bg-lighten02" dir="rtl" style="border-radius: 10px;">
                                    <h4 class="accordion-header color-primary " style="padding: 10px;">
                                        جامعة همدرد، دلهي
                                    </h4>
                                </div>
                                <div class="accordion-item no-collapsed bg-lighten02" dir="rtl" style="border-radius: 10px;">
                                    <h4 class="accordion-header color-primary " style="padding: 10px;">
                                        الجامعة الإسلاميّة للعلوم والتكنولوجيا، كاشمير
                                    </h4>
                                </div>
                                <div class="accordion-item no-collapsed bg-lighten02" dir="rtl" style="border-radius: 10px;">
                                    <h4 class="accordion-header color-primary " style="padding: 10px;">
                                        جامعة مولانا آزاد الأرديّة، حيدر آباد
                                    </h4>
                                </div>


                            </div>

                        </div>








                    </div>



                </div>
            </div>
        </section>





        <!--=====================================-->
        <!--=        Footer Area Start       	=-->
        <!--=====================================-->
        <!-- Start Footer Area  -->
        {{>footer}}
        <!-- End Footer Area  -->
    </div>

    <div class="rn-progress-parent">
        <svg class="rn-back-circle svg-inner" width="100%" height="100%" viewBox="-1 -1 102 102">
            <path d="M50,1 a49,49 0 0,1 0,98 a49,49 0 0,1 0,-98" />
        </svg>
    </div>

